ADD FIELD "OwnerType" OF "address" AS character 
  DESCRIPTION "M)ember, E)mployer, A)gent, C)ompany, D)ependant, O)rganisation, B)roker, K)Courier, P)rovider, T) Disease "
  FORMAT "X"
  INITIAL ""
  LABEL "Owner Type"
  POSITION 2
  MAX-WIDTH 2
  COLUMN-LABEL "T"
  VALEXP "lookup(OwnerType,""M,E"") > 0"
  VALMSG "Only M and E are valid choices"
  HELP "M)ember, E)mployer"
  ORDER 10

