  INITIAL ""
  LABEL "Post code"
  POSITION 7
  MAX-WIDTH 8
  COLUMN-LABEL "Post"
  VALEXP "PostCode = ""0000"" or can-find(first postcode where postcode.BoxCode = PostCode)"
