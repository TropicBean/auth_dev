  FORMAT "999"
  INITIAL "0"
  LABEL "Address Number"
  POSITION 4
  MAX-WIDTH 4
  COLUMN-LABEL "Add#"
  HELP "Address number (0 is the main address)"
  ORDER 30

ADD FIELD "PhysAddress" OF "address" AS character 
  FORMAT "x(120)"
  INITIAL ""
  LABEL "Physical Address"
  POSITION 5
  MAX-WIDTH 240
  VIEW-AS "view-as editor size 30 by 4 buffer-lines 4"
  COLUMN-LABEL "Phys. Address"
  VALEXP "PhysAddress <> """""
  VALMSG "The physical address must be entered"
  HELP "Physical Address"
  ORDER 60

