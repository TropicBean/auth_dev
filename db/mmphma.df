  FORMAT "x(8)"
  INITIAL ""
  LABEL "Product Code"
  POSITION 2
  MAX-WIDTH 16
  COLUMN-LABEL "Prod Code"
  VALEXP "can-find hproduct where hproduct.product-code = product-code"
  VALMSG "This must be a valid product code"
